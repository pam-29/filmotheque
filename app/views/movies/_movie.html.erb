<div id="<%= dom_id movie %>">
    <strong>Image:</strong>
  <p>
    <%= image_tag movie.image, width: 200 %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= movie.title %>
  </p>

  <p>
    <strong>Summary:</strong>
    <%= movie.summary %>
  </p>

  <p>
    <strong>Release year:</strong>
    <%= movie.release_year %>
  </p>

  <p>
    <strong>Actor:</strong>
    <%= movie.actor %>
  </p>

  <p>
    <strong>Director:</strong>
    <%= movie.director %>
  </p>

</div>
